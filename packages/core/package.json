{
  "name": "@tokenxtractor/core",
  "version": "1.0.0",
  "description": "Core pipeline for TokenXtractor: normalizer, redactor, formatter, uploader",
  "author": "tokenxtractor project contributors",
  "license": "MIT",
  "keywords": [
    "claude",
    "codex",
    "coding-agents",
    "ai",
    "dataset",
    "huggingface",
    "github",
    "agent"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts --clean",
    "dev": "tsup src/index.ts --format cjs --dts --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.0",
    "glob": "^10.3.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.1.9"
  }
}
